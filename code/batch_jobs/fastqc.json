{
    "jobName": "fastqc",
    "jobQueue": "bioinformatics-default-queue",
    "jobDefinition": "biotech-fastqc:1",
    "containerOverrides": {
        "command": ["fastqc *.gz"],
        "environment": [{
                "name": "JOB_INPUTS",
                "value": "s3://1000genomes/pilot_data/data/NA12878/pilot3_unrecal/SRR014820_*.fastq.gz"
            },
            {
                "name": "JOB_OUTPUTS",
                "value": "*.html *.zip"
            },
            {
                "name": "JOB_OUTPUT_PREFIX",
                "value": "s3://BUCKET_NAME/output/SRR014820"
            }
        ]
    }
}