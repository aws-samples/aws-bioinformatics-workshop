{
  "containerProperties": {
    "command": [],
    "image": "REPLACE_WITH_IMAGE_FROM_ECR",
    "resourceRequirements": [
      {
        "type": "VCPU",
        "value": "4"
      },
      {
        "type": "MEMORY",
        "value": "8000"
      }
    ],
    "mountPoints": [
      {
        "sourceVolume": "awscli",
        "containerPath": "/opt/aws-cli",
