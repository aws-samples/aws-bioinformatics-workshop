      }
    ],
    "volumes": [
      {
        "name": "awscli",
        "host": {
          "sourcePath": "/opt/aws-cli"
        }
      }
    ]
  },
  "platformCapabilities": [
    "EC2"
  ],
  "tags": {
    "Environment": "Dev",
    "Project": "Biotech"
  },
  "propagateTags": true,
  "type": "container",
  "jobDefinitionName": "biotech-samtools",
  "timeout": {
    "attemptDurationSeconds": 300
  },
  "retryStrategy": {
    "attempts": 1
  }
}